{
	"manifest_version": 3,
	"name": "Job Salary Analyzer",
	"version": "1.0",
	"description": "Analyzes job postings to predict salary ranges using AI",
	"permissions": ["activeTab", "scripting"],
	"action": {
	  "default_popup": "popup.html"
	},
	"content_scripts": [
	  {
		"matches": ["<all_urls>"],
		"js": ["content.js"]
	  }
	]
  }